{
    "manifestVersion": 1,
    "id": "vsts-smoke-web-test-task",
    "name": "Smoke Web Test Task",
    "version": "1.0.0",
    "publisher": "miguelcruz",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "images/icon-small.png"
    },
    "description": "Task for performing smoke web tests.",
    "categories": [
        "Build and release"
    ],
    "files": [
        {
            "path": "SmokeWebTest"
        },
        {
            "path": "images/donate.png",
            "addressable": true
        },
        {
            "path": "images/icon-small.png",
            "addressable": true
        },
        {
            "path": "images/task.jpg",
            "addressable": true
        },
        {
            "path": "images/task-detail.jpg",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "smoke-web-test",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "SmokeWebTest"
            }
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/Duber/vsts-smoke-web-test-task"
    },
    "public": false
}
